{
  "provider": "genai",
  "apiKey": "",
  "baseUrl": "",
  "model": "o3",
  "temperature": 0.7,
  "maxTokens": 4096,
  "planModeApiProvider": "genai",
  "actModeApiProvider": "genai",
  "planModeApiModelId": "o3",
  "actModeApiModelId": "o3",
  "genAiApiKey": "",
  "genAiBaseUrl": "",
  "aiAgent": {
    "enableSystemCommands": true,
    "enableFileOperations": true,
    "enableWebRequests": true,
    "requireApprovalForDangerousCommands": true,
    "commandTimeout": 30000,
    "maxOutputLength": 50000,
    "systemPromptLanguage": "spanish",
    "conversationHistoryLimit": 50,
    "autoExecuteSafeTools": false,
    "streamCommandOutput": true
  },
  "security": {
    "allowedCommands": [
      "ls", "dir", "pwd", "cd", "cat", "type", "head", "tail", "find", "grep",
      "ps", "top", "df", "du", "whoami", "date",
      "curl", "wget", "nc", "netcat", "telnet", "ping", "traceroute",
      "nmap", "nikto", "dirb", "gobuster", "ffuf", "wfuzz",
      "sqlmap", "hydra", "john", "hashcat",
      "burpsuite", "zaproxy", "nuclei", "subfinder", "amass",
      "git", "python", "pip", "node", "npm", "java", "mvn"
    ],
    "blockedCommands": [
      "rm -rf",
      "del /f",
      "format",
      "fdisk",
      "mkfs",
      "sudo rm",
      "sudo dd",
      "chmod 777",
      "chown -R"
    ],
    "allowedDomains": [
      "*",
      "api.github.com", "registry.npmjs.org", "pypi.org", "maven.apache.org",
      "cve.mitre.org", "nvd.nist.gov", "exploit-db.com", "packetstormsecurity.com",
      "vulndb.cyberriskanalytics.com", "cvedetails.com"
    ],
    "maxFileSizeBytes": 10485760,
    "allowedFileExtensions": [
      ".js", ".ts", ".jsx", ".tsx", ".json", ".md", ".txt",
      ".py", ".java", ".go", ".rs", ".cpp", ".c", ".h",
      ".php", ".asp", ".aspx", ".jsp", ".rb", ".pl", ".sh",
      ".css", ".scss", ".html", ".xml", ".yaml", ".yml",
      ".config", ".conf", ".ini", ".env", ".properties",
      ".sql", ".db", ".sqlite", ".log"
    ]
  }
}
